<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.prieto.countbook">

    <application
        android:name=".CountBookApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="jessicaa-CountBook"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.CounterListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.AddCounterActivity"
            android:parentActivityName=".View.CounterListActivity" />
        <activity
            android:name=".View.CounterActivity"
            android:parentActivityName=".View.CounterListActivity" />
        <activity android:name=".View.CounterEditActivity" />
        <activity android:name=".View.CounterDetailsActivity"></activity>
    </application>

</manifest>